{
	"curly": true,
	"esnext": true,
	"eqeqeq": true,
	"forin": true,
	"immed": true,
	"jquery": true,
	"latedef": true,
	"laxcomma": true,
	"newcap": true,
	"noarg": true,
	"node": true,
	"noempty": true,
	"nonew": true,
	"regexp": true,
	"smarttabs": true,
	"strict": true,
	"trailing": true,
	"undef": true,
	"unused": true,



	"maxparams": 10,
	"maxdepth": 3,
	"maxstatements": 25,
	"maxlen": 140,

    "browser": true,
	"devel": true,

    "predef": [ "define", "require", "requirejs", "_", "jasmine", "beforeEach", "afterEach", "it", "expect",
		"describe", "ddescribe", "iit", "xit", "confirm", "spyOn", "waitsFor", "module", "inject", "log"]
}
